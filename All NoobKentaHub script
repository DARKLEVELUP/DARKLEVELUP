local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Noob Kenta HUB", "GrapeTheme")
local Tab = Window:NewTab("Script")
local Section = Tab:NewSection("Script")
local Keybind = Window:NewTab("Misc")
local SectionKeybind = Keybind:NewSection("Keybind")


Section:NewButton("Anime Evoulution Simulator", "", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/DARKLEVELUP/DARKLEVELUP/main/Anime%20Evolution%20Simulator"))()
end)



Section:NewButton("Anime Evoulution Simulator V2", "", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/DARKLEVELUP/DARKLEVELUP/main/Anime%20Evolution%20Simulator%20V2"))()
end)




Section:NewButton("Anime Fighting Simulator", "", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/DARKLEVELUP/DARKLEVELUP/main/Anime%20Fighting"))()
end)



Section:NewButton("Idles Heros", "", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/DARKLEVELUP/DARKLEVELUP/main/Idles%20Heros"))()
end)



Section:NewButton("General Script", "", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/DARKLEVELUP/DARKLEVELUP/main/General%20Script"))()
end)


SectionKeybind:NewKeybind("RightControl_to_hide_UI", "KeybindInfo", Enum.KeyCode.RightControl, function()
	Library:ToggleUI()
end)
